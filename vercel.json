{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/",
      "destination": "/frontend/index.html"
    },
    {
      "source": "/:path((?!api/).*)",
      "destination": "/frontend/:path"
    }
  ],
  "functions": {
    "api/parking.js": {
      "includeFiles": "backend/data/**"
    }
  }
}